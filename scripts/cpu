COLOR_FG_GREEN='\033[0;32m'
COLOR_RESET='\033[0m'
do_cmd ()
{
    local cmd_args="";
    for arg in "$@";
    do
        case "$arg" in
            *\ *)
                cmd_args="$cmd_args \"$arg\""
            ;;
            *)
                cmd_args="$cmd_args $arg"
            ;;
        esac;
    done;
    echo -e "${COLOR_FG_GREEN}($cmd_args )${COLOR_RESET}" 1>&2;
    command "$@"
}
if [ "$(uname -s)" == "Darwin" ]
then
  do_cmd top -F -R -o cpu
else
  do_cmd top -o +%CPU
fi
