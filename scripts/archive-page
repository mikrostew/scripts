requirement_check ()
{
    local cmd="$1";
    local how_to_install="$2";
    if [ ! $(command -v $cmd) ]; then
        echo_err "[ERROR] Command '$cmd' is required for this script, but not installed";
        echo_err "To install: $how_to_install";
        return 1;
    else
        return 0;
    fi
}
combined_return=0
requirement_check wget "search 'how to install wget'"
combined_return=$(( combined_return + $? ))
if [ "$combined_return" != 0 ]; then exit $combined_return; fi
page_url="${1:?No URL entered}"
wget --wait=1 --random-wait --convert-links --page-requisites --user-agent="" "$page_url"
