#!/usr/bin/env bash
###########################################################################
# DO NOT EDIT! This script was auto-generated. To update this script, edit
# the file script-gen/new_chrome, and run './generate-scripts.sh'
###########################################################################

clipboard_contents="$(pbpaste)"
if [[ "$clipboard_contents" =~ ^http ]]
then
  osascript <<END_SCRIPT
tell application "Google Chrome"
  make new window
  set URL of active tab of window 1 to "$clipboard_contents"
  activate
end tell
END_SCRIPT
else
  osascript -e 'tell application "Google Chrome" to make new window activate'
fi
